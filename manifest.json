{
  "manifest_version": 3,
  "name": "ALL Tab URL Exporter",
  "version": "1.0.0",
  "description": "現在のウィンドウで開いているすべてのタブのURLを取得・エクスポートします",
  "permissions": [
    "tabs",
    "clipboardWrite"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
